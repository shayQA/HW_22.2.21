package com.company;

public class NumberTranslator {

    public String translateNumber(int num)
    {
        switch (num)
        {
            case 1:
                return "one";
            case 2:
                return "two";
            case 3:
                return "three";
            case 4:
                return "four";
            case 5:
                return "five";
            default:
                return "number is incorrect";
        }
    }
}
